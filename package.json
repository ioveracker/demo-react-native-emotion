{
  "name": "demo-react-native-emotion",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "yarn setup:ios",
    "setup:ios": "cd ios && rbenv install --skip-existing && bundle install && bundle exec pod install",
    "setup:ruby": "cd ios && rbenv install --skip-existing && yarn setup:bundler && rbenv rehash",
    "setup:bundler": "gem install bundler -v 2.0.1 --force",
    "clean": "rm -rf node_modules ios/Pods",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier:project": "prettier --write '**/*.{js,ts,tsx}'"
  },
  "dependencies": {
    "@emotion/native": "11.0.0-next.15",
    "@emotion/react": "11.0.0-next.15",
    "react": "16.13.1",
    "react-native": "0.63.2"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.13",
    "@types/react-native": "0.63.13",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-jest": "25.5.1",
    "eslint": "6.8.0",
    "jest": "25.5.4",
    "metro-react-native-babel-preset": "0.63.0",
    "prettier": "2.1.1",
    "react-test-renderer": "16.13.1",
    "typescript": "4.0.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
